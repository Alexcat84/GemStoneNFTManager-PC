{
  "version": 2,
  "builds": [
    {
      "src": "index-simple.js",
      "use": "@vercel/node"
    },
    {
      "src": "admin-panel/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index-simple.js"
    },
    {
      "src": "/",
      "dest": "/index-simple.js"
    },
    {
      "src": "/dashboard",
      "dest": "/index-simple.js"
    },
    {
      "src": "/qr/(.*)",
      "dest": "/index-simple.js"
    },
    {
      "src": "/qr-codes/(.*)",
      "dest": "/index-simple.js"
    },
    {
      "src": "/(.*)",
      "dest": "/admin-panel/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
