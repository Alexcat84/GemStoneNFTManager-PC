{
  "version": 2,
  "builds": [
    {
      "src": "04-main-website/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "04-main-website/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/gallery",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/faq",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/shipping",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/returns",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/privacy-policy",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/terms-of-service",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/nft-guide",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/04-main-website/public/$1"
    }
  ]
}