{
  "version": 2,
  "builds": [
    {
      "src": "04-main-website/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "04-main-website/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "04-main-website/admin-panel/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/04-main-website/admin-panel/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/04-main-website/index.js"
    }
  ]
}
