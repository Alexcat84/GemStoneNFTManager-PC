{
  "version": 2,
  "builds": [
    {
      "src": "04-main-website/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "04-main-website/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/gallery",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/04-main-website/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/04-main-website/index.js"
    }
  ]
}