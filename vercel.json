{
  "version": 2,
  "buildCommand": "cd 04-main-website && npm install",
  "outputDirectory": "04-main-website/public",
  "installCommand": "cd 04-main-website && npm install",
  "functions": {
    "04-main-website/index.js": {
      "runtime": "@vercel/node@3.0.12"
    }
  },
  "routes": [
    {
      "src": "/test",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/admin/login",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/admin/debug",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/admin/dashboard",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/04-main-website/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/04-main-website/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self' https:;"
        }
      ]
    }
  ]
}
